<h1 align="center"> 
	Welcome to <%= @user.name %> page
</h1>


<div class="row">
	<div class="col-md-8 col-md-offset-2 center">
    <% if @user.user_type == "Manager" %>
				<%= render 'projects/project', obj: @user.managed_projects %>
    <% elsif @user.user_type == "Developer" %>      
        <%= render 'projects/project', obj: @user.projects %>
     <% else %>
        <%= render 'bugs/bug', bugs: @user.bugs %>
     <% end %>   
			
		
	</div>
</div>
