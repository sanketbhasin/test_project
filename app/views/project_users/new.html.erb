<div align="center">
	<h1> <%= @project.title %> </h1>
</div>

<div class="col-md-4 col-xs-4 col-xs-offset-4 col-md-offset-4">
  <%= form_for(@project_user,:url => project_project_users_path, :html => {class: "form-vertical", role: "form"} ) do |f| %>

	
	<div class="form-group">
      <div class="control-label">
        <%= f.label "Developer List" %>
      </div>
     
      <%= f.select :user_id, options_for_select(@users.collect { |player| ["#{player.name}", player.id] }), { :include_blank => false },  {:class => 'form-control'} %>
     
    </div>

	<%= f.submit("Add", class: 'btn btn-primary btn-lg') %>
	<%end %>



</div>